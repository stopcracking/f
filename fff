							end

							walkto = true
							spawn(function ()
								repeat wait()
									p.Character:FindFirstChildOfClass('Humanoid'):MoveTo(Player.Character.HumanoidRootPart.Position)
								until Player.Character == nil or not Player.Character.HumanoidRootPart or walkto == false
							end)
						end
					end
