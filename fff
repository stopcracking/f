function r15(p)
	if p.Character:FindFirstChildOfClass('Humanoid').RigType == Enum.HumanoidRigType.R15 then
		return true
	end
end

function getTorso(x)
	x = x or game.Players.LocalPlayer.Character
	return x:FindFirstChild("Torso") or x:FindFirstChild("UpperTorso") or x:FindFirstChild("LowerTorso") or x:FindFirstChild("HumanoidRootPart")
end


function drop()
	p.Character.Humanoid:UnequipTools()
	wait(0.3)
	for i, v in pairs(p.Backpack:GetChildren()) do
		if v:IsA("Tool") and v.Name == radio then
			v.Parent = p.Character
			v.Parent = workspace
		end
	end
end

function ToOwner()
	p.Character.Animate.Disabled = true
	local Offet = CFrame.new(-1.1, -0.5, 1.1)
	teleportLoop = game.RunService.Stepped:Connect(function()
	   pcall(function()
		  p.Character:FindFirstChildOfClass("Humanoid").Sit = false
		  game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = Players[owner].Character.HumanoidRootPart.CFrame * Offet
	   end)
	end)
end

function UnToOwner()
	teleportLoop = teleportLoop:Disconnect()
end

function giveradioowner()
	ToOwner()
	wait(0.5)
	p.Character.Humanoid:UnequipTools()
	for i, v in pairs(p.Backpack:GetChildren()) do
		if v:IsA("Tool") and v.Name == radio then
			v.Parent = p.Character
			v.Parent = workspace
		end
	end
	UnToOwner()
end

function grabtools()
    Bd()
    local Human = p.Character:FindFirstChildWhichIsA("Humanoid")
    for _, v in ipairs(workspace:GetChildren()) do
        if p.Character and v:IsA("BackpackItem") and v:FindFirstChild("Handle") then
            Human:EquipTool(v)
        end
    end
    if grabtoolsFunc then grabtoolsFunc:Disconnect() end
    grabtoolsFunc = workspace.ChildAdded:Connect(function(v)
        if p.Character and v:IsA("BackpackItem") and v:FindFirstChild("Handle") then
            p.Character.Humanoid:EquipTools()
        end
    end)
end
function ungrabtools()
    if grabtoolsFunc then grabtoolsFunc:Disconnect() end
end
function Bd()
    game:GetService("ReplicatedStorage").HDAdminClient.Signals.ChangeSetting:InvokeServer("Prefix", ";")
end
function grabtoolsowner()
	if getgenv().Owner == p.UserId then
		grabtools()
		wait(2)
		ungrabtools()
	end
end

function hhelloo()
	if getgenv().Owner == p.UserId then
		grabtoolsowner()
		wait(0.4)
		giveradioowner()
	end
end

function Dupe(amount)
    if tonumber(amount) or 1 then
    local DropT = {}
    p.Character.Humanoid:UnequipTools()
    stopDupe = false
    local Pos = p.Character.HumanoidRootPart.CFrame
    p.Character:MoveTo(Vector3.new(0, 1e7, 0))
    wait(.15)
    p.Character.HumanoidRootPart.Anchored = true
    for i = 1, amount do
        if stopDupe then
            break
        end
        wait(.2)
        for i, v in pairs(p.Backpack:GetChildren()) do
            if v:IsA("Tool") and v.Name == radio then
                v.Parent = p.Character
                v.Parent = workspace
                table.insert(DropT, v)
            end
        end
        wait(.2)
        Bd()
        repeat 
            game:GetService("ReplicatedStorage").HDAdminClient.Signals.RequestCommand:InvokeServer(";refresh ") 
			p.Character:WaitForChild("Humanoid").Health = 0
			wait()
        until p.CharacterAdded:wait()
        if stopDupe then
            break
        end
    end
    wait(0.2)
    p.Character:WaitForChild("Humanoid"):UnequipTools()
    wait(1)
    for i, v in pairs(workspace:GetChildren()) do
    if v:IsA("Tool") and v.Name == radio then
        firetouchinterest(v.Handle, p.Character.HumanoidRootPart, 0)
        p.Character.HumanoidRootPart.CFrame = Pos
    end
    end
    grabtools()
    wait(0.5)
    ungrabtools()
wait(1)
hhelloo()
end
end

function Equip()
    for i, v in pairs(p.Backpack:GetChildren()) do
        if v:IsA("Tool") and v.Name == radio then
            wait(.01)
            v.Parent = p.Character
        end
    end
end

