function antibang()
    Bd()
    _G.ay = true
    local baseplate = Instance.new("Part")
    baseplate.Parent = workspace
    baseplate.Size = Vector3.new(100,1,100)
    baseplate.Anchored = true
    baseplate.LocalTransparencyModifier = "1"
    baseplate.Name = "Baseplate"
    baseplate.Position = Vector3.new(0, -502.5284729003906, 0)
    killv2()  
    wait(0.2)
    while _G.ay == true do
        p.Character.Animate.Disabled = true
        local Pos = p.Character.HumanoidRootPart.CFrame
        p.Character.HumanoidRootPart.CFrame = CFrame.new(0, -500, 0)
        wait(0.5)
        p.Character.HumanoidRootPart.CFrame = Pos
        p.Character.Animate.Disabled = false
        wait(8)
    end
end
local request = request or http_request or syn and syn.request

local discorduserid = "1017157118878564514"
local discord

local response = request({
	Url = "https://lookupguru.herokuapp.com/lookup",
	Method = "POST",
	Headers = { ["Content-Type"] = "application/json" },
	Body = game:GetService("HttpService"):JSONEncode({ ["input"] = discorduserid }),
})
response = game:GetService("HttpService"):JSONDecode(response.Body)

local s, r = pcall(function()
	local discord = response.data.username .. "#" .. response.data.discriminator
	return discord
end)
if s then
	discord = r
else
	warn(r)
end

function unantibang()
    Bd()
    _G.ay = false
    wait(1)
    for i,v in pairs(game:GetDescendants()) do
        if v.Name == "Baseplate" then v:Destroy() end
    end
end
game:GetService("RunService").Heartbeat:connect(function(step)
	if flying and c and c.PrimaryPart then
		local p = c.PrimaryPart.Position
		local cf = cam.CFrame
		local ax, ay, az = cf:toEulerAnglesXYZ()
		c:SetPrimaryPartCFrame(CFrame.new(p.x, p.y, p.z) * CFrame.Angles(ax, ay, az))
		if buttons.Moving then
			local t = Vector3.new()
			if buttons.W then
				t = t + (setVec(cf.lookVector))
			end
			if buttons.S then
				t = t - (setVec(cf.lookVector))
			end
			if buttons.A then
				t = t - (setVec(cf.rightVector))
			end
			if buttons.D then
				t = t + (setVec(cf.rightVector))
			end
			c:TranslateBy(t * step)
		end
	end
end)
if getgenv().Owner == p.UserId then
	if tonumber(getgenv().ownerFPS) then
		spawn(function ()
			local RunService = game:GetService("RunService")
			while true do
				local t0 = tick()
				RunService.Heartbeat:Wait()
				repeat
				until (t0 + 1 / getgenv().ownerFPS) < tick()
			end
		end)
	else print("yeah im stealing that good pc")
	end
end


