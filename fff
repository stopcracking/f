					end
					if fainalMsg == getgenv().prefix .. "stop following " .. p.Name:lower() then
						walkto = false
					end
					if fainalMsg == getgenv().prefix .. "stop following" then
						walkto = false
					end
					if finalMsg == getgenv().prefix .. "setup" then
						setup()
					end
					if finalMsg == getgenv().prefix .. "antikill " .. p.Name:lower() then
						game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer("antikill is active ;)", "All")
						antikill()
					end
					if finalMsg == getgenv().prefix .. "antiafk" then
						game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer("antiafk is already active ;)", "All")
					end
					if finalMsg == getgenv().prefix .. "antikill" then
						game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer("antikill is active ;)", "All")
						antikill()
					end
					if finalMsg == getgenv().prefix .. "antibang" then
						game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer("antibang is active ;)", "All")
						antibang()
					end
					if finalMsg == getgenv().prefix .. "antibang" .. p.Name:lower() then
						game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer("antibang is active ;)", "All")
						antibang()
					end
					if finalMsg == getgenv().prefix .. "unantibang" then
						game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer("antibang disabled ;)", "All")
						unantibang()
					end
					if finalMsg == getgenv().prefix .. "inv" then
						inv()
					end
					if finalMsg == getgenv().prefix .. "unantibang" .. p.Name:lower() then
						game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer("antibang disabled ;)", "All")
